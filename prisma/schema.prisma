generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Player {
  id            Int     @id @default(autoincrement())
  name          String   @unique
  description   String?
  team          Team     @relation(fields: [teamId], references: [id])
  teamId        Int
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model Team {
  id      Int      @id @default(autoincrement())
  name    String
  players Player[]
}
